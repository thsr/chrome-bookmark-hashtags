{
  "name": "Hashtags for Chrome bookmarks",
  "description": "Add #hashtags to your Chrome bookmarks and manage them easily!",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/new.png",
    "128": "icons/new.png"
  },
  "browser_action": {
    "default_title": "Hashtags for Chrome bookmarks"
  },
  "background": {
    "scripts": ["background.js"],
     "persistent": false
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "tabs"
  ],
  "options_ui": {
    "page": "options/options.html"
  }
}
